<%= form_tag("/users", method: 'POST') do %>
  <%= label_tag(:username) %>
  <%= text_field_tag(:username) %>
  <br>

  <%= label_tag(:email) %>
  <%= email_field_tag(:email) %>
  <br>

  <%= label_tag(:password) %>
  <%= password_field_tag(:password) %>
  <br>

  <%= submit_tag('Create') %>
<% end %>